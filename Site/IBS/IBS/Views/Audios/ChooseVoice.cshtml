        function play(id) {
            var audio = document.getElementById(id);
            var audioId = id.split("_");
            $('#play_' + audioId[1]).css('display', 'none');
            $('#pause_' + audioId[1]).css('display', 'block');
            audio.play();
        }

        function pause(id) {
            var audio = document.getElementById(id);
            var audioId = id.split("_");
            $('#play_' + audioId[1]).css('display', 'block');
            $('#pause_' + audioId[1]).css('display', 'none');
            audio.pause();
        }


